{
    "name": "presttec\/laravel-cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": [
        "laravel",
        "braintree",
        "billing",
        "cashier"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "presttec\/illuminate-database": "~5.3",
        "presttec\/illuminate-support": "~5.3",
        "presttec\/braintree_php": "~3.0",
        "presttec\/dompdf": "^0.7.0",
        "presttec\/carbon": "~1.0",
        "presttec\/http-kernel": "~2.7|~3.0"
    },
    "require-dev": {
        "presttec\/illuminate-routing": "~5.3",
        "presttec\/illuminate-http": "~5.3",
        "presttec\/illuminate-view": "~5.3",
        "presttec\/mockery": "~0.9",
        "presttec\/phpunit": "~4.0",
        "presttec\/phpdotenv": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/cashier-braintree": "*"
    },
    "conflicts": {
        "laravel\/cashier-braintree": "*"
    }
}